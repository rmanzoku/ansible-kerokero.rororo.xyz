	    "Action": [
		"s3:ListAllMyBuckets"
	    ],
	    "Resource": [
		"arn:aws:s3:::*"
	    ]
	},
	{
	    "Effect": "Allow",
	    "Action": [
		"s3:*"
	    ],
	    "Resource": [
		"arn:aws:s3:::img.prod.s3.kerokero.rororo.xyz",
                "arn:aws:s3:::img.prod.s3.kerokero.rororo.xyz/*"
	    ]
	}
    ]
}
